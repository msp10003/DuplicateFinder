<Window x:Class="DuplicateFinderGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="Source_Spreadsheet_Path" HorizontalAlignment="Left" Height="10" Margin="57,42,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="187" TextChanged="Source_Spreadsheet_Path_TextChanged"/>
        <TextBox x:Name="Destination_Spreadsheet_Path" HorizontalAlignment="Left" Height="10" Margin="286,42,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="187" TextChanged="TextBox_TextChanged"/>
        <Button x:Name="Execute" Content="EXECUTE" HorizontalAlignment="Left" Margin="339,237,0,0" VerticalAlignment="Top" Width="75" Click="Execute_Click"/>
        <Slider x:Name="Tolerance_Slider" HorizontalAlignment="Left" Height="125" Margin="154,94,0,0" VerticalAlignment="Top" Width="17" ValueChanged="Tolerance_Slider_ValueChanged"/>

    </Grid>
</Window>
