<Window x:Name="Duplicate_Finder" x:Class="DuplicateFinderGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Duplicate Finder" Height="350" Width="525" Background="#FF56A444">
    <Grid Background="#FF908181">
        <TextBox x:Name="Source_Spreadsheet_Path" HorizontalAlignment="Left" Height="20" Margin="57,42,0,0" TextWrapping="Wrap" Text="Path name..." VerticalAlignment="Top" Width="193" TextChanged="Source_Spreadsheet_Path_TextChanged" FontFamily="Arial"/>
        <TextBox x:Name="Destination_Spreadsheet_Path" HorizontalAlignment="Left" Height="20" Margin="286,42,0,0" TextWrapping="Wrap" Text="Path name..." VerticalAlignment="Top" Width="198" TextChanged="Destination_Spreadsheet_TextChanged" FontFamily="Arial"/>
        <Button x:Name="Execute" Content="EXECUTE" HorizontalAlignment="Left" Margin="433,292,0,0" VerticalAlignment="Top" Width="75" Click="Execute_Click" FontFamily="Arial"/>
        <Button x:Name="Clear" Content="CLEAR" HorizontalAlignment="Left" Margin="353,292,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" FontFamily="Arial"/>
        <Label Content="Source Spreadsheet Path" HorizontalAlignment="Left" Margin="57,62,0,0" VerticalAlignment="Top" FontFamily="Arial"/>
        <Label Content="Output Spreadsheet Path" HorizontalAlignment="Left" Margin="286,62,0,0" VerticalAlignment="Top" FontFamily="Arial"/>

    </Grid>
</Window>
